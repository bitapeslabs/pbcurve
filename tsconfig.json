  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "strictFunctionTypes": true,

    "module": "CommonJS",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
